{
  "version": 3,
  "routes": [
    {
      "src": "^/_astro/(.*)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    }
  ],
  "images": {
    "sizes": [320, 640, 768, 1024, 1280, 1536, 1920],
    "formats": ["image/avif", "image/webp"],
    "domains": [],
    "remotePatterns": []
  }
}
